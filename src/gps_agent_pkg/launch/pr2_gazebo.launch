<launch>
	<include file="$(find gazebo_worlds)/launch/empty_world.launch">
        <arg name="use_sim_time" value="false"/>
    </include>
	<include file="$(find pr2_gazebo)/launch/pr2_no_controllers.launch" />
    <param name="PR2Plugin/type" value="gps_agent_pkg/PR2Plugin" />
    <!-- rosparam command="load" file="$(find gps_agent_pkg)/launch/pr2_head_controller.yaml" -->
    <rosparam command="load" file="$(find pr2_controller_configuration_gazebo)/pr2_head_controller.yaml" />
    <!-- Controllers that come up started -->
    <node name="controller_spawner"
         pkg="pr2_controller_manager" type="pr2_controller_manager" output="screen"
         args="spawn PR2Plugin" />

</launch>
